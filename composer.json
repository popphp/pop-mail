{
    "name": "popphp/pop-mail",
    "description": "Pop Mail Component for Pop PHP Framework",
    "keywords": [
        "mail",
        "send mail",
        "mail queue",
        "php",
        "pop",
        "pop php"
    ],
    "homepage": "https://github.com/popphp/pop-mail",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@noladev.com",
            "homepage": "https://www.popphp.org/"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "popphp/pop-http": "^5.3.2",
        "popphp/pop-mime": "^2.0.2",
        "popphp/pop-utils": "^2.2.0",
        "google/apiclient": "^v2.17.0",
        "aws/aws-sdk-php": "^3.316.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.0"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Mail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Mail\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
