{
    "name": "popphp/pop-mail",
    "description": "Pop Mail Component for Pop PHP Framework",
    "keywords": [
        "mail",
        "send mail",
        "mail queue",
        "php",
        "pop",
        "pop php"
    ],
    "homepage": "http://www.popphp.org/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@nolainteractive.com",
            "homepage": "http://www.nolainteractive.com/"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "popphp/pop-http": "^5.1.1",
        "popphp/pop-mime": "^2.0.0",
        "popphp/pop-utils": "^2.1.0",
        "google/apiclient": "^v2.15.1",
        "aws/aws-sdk-php": "^3.283.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0.0"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Mail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Mail\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
